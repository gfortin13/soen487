<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from XMLReaderService WSDL service -->

  <service name="XMLReader" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://XMLUtils.util"/>
      <parameter name="wsdlServiceElement" value="XMLReaderService"/>
      <parameter name="schemaQualified" value="http://XMLUtils.util"/>
      <parameter name="wsdlServicePort" value="XMLReader"/>
      <parameter name="className" value="util.XMLUtils.XMLReaderSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="XMLReader"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="read" qname="operNS:read" xmlns:operNS="http://XMLUtils.util" soapAction="" >
        <parameter qname="pns:url" xmlns:pns="http://XMLUtils.util" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:type" xmlns:pns="http://XMLUtils.util" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="read"/>

  </service>
</deployment>
